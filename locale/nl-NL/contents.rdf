<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the locales being supplied by this jar -->
  <RDF:Seq about="urn:mozilla:locale:root">
    <RDF:li resource="urn:mozilla:locale:nl-NL"/>
  </RDF:Seq>

  <!-- list packages for which locale information is provided -->
   <RDF:Description about="urn:mozilla:locale:nl-NL"
       chrome:name="nl-NL"">
    <chrome:packages>
      <RDF:Seq about="urn:mozilla:locale:nl-NL:packages">
        <RDF:li resource="urn:mozilla:locale:nl-NL:mailredirect"/>
      </RDF:Seq>
    </chrome:packages>
  </RDF:Description>

</RDF:RDF>
