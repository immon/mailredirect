{
  "extensionName": {
    "message": "Mail Redirect",
    "description": "If you do translate this string, please append the original add-on name in parenthesis: (Mail Redirect)"
  },
  "extensionDescription": {
    "message": "Allow to redirect (a.k.a. \"remail\") mail messages to other recipients."
  },

  "emptyMailredirectWindowTitle": {
    "message": "Mail Redirect"
  },
  "mailredirectWindowTitle": {
    "message": "Mail Redirect: $MSG$ - $APP$",
    "placeholders": {
      "msg": {
        "content": "$1",
        "example": "Subject of the message."
      },
      "app": {
        "content": "$2",
        "example": "Thunderbird"
      }
    }
  },

  "pluralRule": {
    "description": "See: https://developer.mozilla.org/en/Localization_and_Plurals",
    "message": "1"
  }
}
